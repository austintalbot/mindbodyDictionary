# Example configuration file for OpenTofu variables
# Copy this file to terraform.tfvars and fill in your actual values

# Azure Configuration
azure_subscription_id           = "49fbd6b5-f722-420c-a6b1-961f1b03813c"
resource_group_name             = "rg-mindbody-notifications"
location                        = "eastus"
notification_hub_namespace_name = "nhn-mindbody"
notification_hub_name           = "nh-mindbody"
notification_hub_sku            = "Free"

# APNS Configuration (iOS)
apns_application_mode = "Sandbox"
apns_bundle_id        = "com.yourcompany.mindbodydictionary"
apns_key_id           = "your-apns-key-id"
apns_team_id          = "your-apple-team-id"
apns_token            = "your-apns-p8-token-content"

# Firebase Cloud Messaging v1 credentials
# Get these from your Firebase project's service account JSON file
# See: https://firebase.google.com/docs/cloud-messaging/migrate-v1
fcm_private_key    = "-----BEGIN PRIVATE KEY-----\nYour private key here\n-----END PRIVATE KEY-----"
fcm_client_email   = "firebase-adminsdk-xxxxx@your-project-id.iam.gserviceaccount.com"
fcm_project_id     = "your-firebase-project-id"
